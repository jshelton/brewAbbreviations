function __brewAbbreviations.init
  function __brewAbbreviations.create_abbr -d "Create Git plugin abbreviation"
    set -l name $argv[1]
    set -l body $argv[2..-1]
    abbr -a $name $body
    set -a __brewAbbreviations_plugin_abbreviations $name
  end

  set -q __brewAbbreviations_plugin_initialized; and exit 0

  set -U __brewAbbreviations_plugin_abbreviations

  # git abbreviations
  __brewAbbreviations.create_abbr g          git
  __brewAbbreviations.create_abbr ga         git add
  __brewAbbreviations.create_abbr gaa        git add --all
  __brewAbbreviations.create_abbr gapa       git add --patch
  __brewAbbreviations.create_abbr gap        git apply
  __brewAbbreviations.create_abbr gb         git branch -vv
  __brewAbbreviations.create_abbr gba        git branch -a -v
  __brewAbbreviations.create_abbr gban       git branch -a -v --no-merged
  __brewAbbreviations.create_abbr gbd        git branch -d
  __brewAbbreviations.create_abbr gbD        git branch -D
  __brewAbbreviations.create_abbr gbl        git blame -b -w
  __brewAbbreviations.create_abbr gbs        git bisect
  __brewAbbreviations.create_abbr gbsb       git bisect bad
  __brewAbbreviations.create_abbr gbsg       git bisect good
  __brewAbbreviations.create_abbr gbsr       git bisect reset
  __brewAbbreviations.create_abbr gbss       git bisect start
  __brewAbbreviations.create_abbr gc         git commit -v
  __brewAbbreviations.create_abbr gc!        git commit -v --amend
  __brewAbbreviations.create_abbr gcn!       git commit -v --no-edit --amend
  __brewAbbreviations.create_abbr gca        git commit -v -a
  __brewAbbreviations.create_abbr gca!       git commit -v -a --amend
  __brewAbbreviations.create_abbr gcan!      git commit -v -a --no-edit --amend
  __brewAbbreviations.create_abbr gcv        git commit -v --no-verify
  __brewAbbreviations.create_abbr gcav       git commit -a -v --no-verify
  __brewAbbreviations.create_abbr gcav!      git commit -a -v --no-verify --amend
  __brewAbbreviations.create_abbr gcm        git commit -m
  __brewAbbreviations.create_abbr gcam       git commit -a -m
  __brewAbbreviations.create_abbr gscam      git commit -S -a -m
  __brewAbbreviations.create_abbr gcfx       git commit --fixup
  __brewAbbreviations.create_abbr gcf        git config --list
  __brewAbbreviations.create_abbr gcl        git clone
  __brewAbbreviations.create_abbr gclean     git clean -di
  __brewAbbreviations.create_abbr gclean!    git clean -dfx
  __brewAbbreviations.create_abbr gclean!!   "git reset --hard; and git clean -dfx"
  __brewAbbreviations.create_abbr gcount     git shortlog -sn
  __brewAbbreviations.create_abbr gcp        git cherry-pick
  __brewAbbreviations.create_abbr gcpa       git cherry-pick --abort
  __brewAbbreviations.create_abbr gcpc       git cherry-pick --continue
  __brewAbbreviations.create_abbr gd         git diff
  __brewAbbreviations.create_abbr gdca       git diff --cached
  __brewAbbreviations.create_abbr gds        git diff --stat
  __brewAbbreviations.create_abbr gdsc       git diff --stat --cached
  __brewAbbreviations.create_abbr gdw        git diff --word-diff
  __brewAbbreviations.create_abbr gdwc       git diff --word-diff --cached
  __brewAbbreviations.create_abbr gdto       git difftool
  __brewAbbreviations.create_abbr gignore    git update-index --assume-unchanged
  __brewAbbreviations.create_abbr gf         git fetch
  __brewAbbreviations.create_abbr gfa        git fetch --all --prune
  __brewAbbreviations.create_abbr gfm        "git fetch origin (__brewAbbreviations.default_branch) --prune; and git merge FETCH_HEAD"
  __brewAbbreviations.create_abbr gfo        git fetch origin
  __brewAbbreviations.create_abbr gl         git pull
  __brewAbbreviations.create_abbr gll        git pull origin
  __brewAbbreviations.create_abbr glr        git pull --rebase
  __brewAbbreviations.create_abbr glg        git log --stat --max-count=10
  __brewAbbreviations.create_abbr glgg       git log --graph --max-count=10
  __brewAbbreviations.create_abbr glgga      git log --graph --decorate --all
  __brewAbbreviations.create_abbr glo        git log --oneline --decorate --color
  __brewAbbreviations.create_abbr glog       git log --oneline --decorate --color --graph
  __brewAbbreviations.create_abbr glom       git log --oneline --decorate --color \(__brewAbbreviations.default_branch\)..
  __brewAbbreviations.create_abbr glod       git log --oneline --decorate --color develop..
  __brewAbbreviations.create_abbr gloo       "git log --pretty=format:'%C(yellow)%h %Cred%ad %Cblue%an%Cgreen%d %Creset%s' --date=short"
  __brewAbbreviations.create_abbr gm         git merge
  __brewAbbreviations.create_abbr gmt        git mergetool --no-prompt
  __brewAbbreviations.create_abbr gp         git push
  __brewAbbreviations.create_abbr gp!        git push --force-with-lease
  __brewAbbreviations.create_abbr gpo        git push origin
  __brewAbbreviations.create_abbr gpo!       git push --force-with-lease origin
  __brewAbbreviations.create_abbr gpv        git push --no-verify
  __brewAbbreviations.create_abbr gpv!       git push --no-verify --force-with-lease
  __brewAbbreviations.create_abbr ggp!       ggp --force-with-lease
  __brewAbbreviations.create_abbr gpu        ggp --set-upstream
  __brewAbbreviations.create_abbr gr         git remote -vv
  __brewAbbreviations.create_abbr gra        git remote add
  __brewAbbreviations.create_abbr grb        git rebase
  __brewAbbreviations.create_abbr grba       git rebase --abort
  __brewAbbreviations.create_abbr grbc       git rebase --continue
  __brewAbbreviations.create_abbr grbi       git rebase --interactive
  __brewAbbreviations.create_abbr grbm       git rebase \(__brewAbbreviations.default_branch\)
  __brewAbbreviations.create_abbr grbmi      git rebase \(__brewAbbreviations.default_branch\) --interactive
  __brewAbbreviations.create_abbr grbmia     git rebase \(__brewAbbreviations.default_branch\) --interactive --autosquash
  __brewAbbreviations.create_abbr grbd       git rebase develop
  __brewAbbreviations.create_abbr grbdi      git rebase \(__brewAbbreviations.default_branch\) --interactive
  __brewAbbreviations.create_abbr grbdia     git rebase \(__brewAbbreviations.default_branch\) --interactive --autosquash
  __brewAbbreviations.create_abbr grbs       git rebase --skip
  __brewAbbreviations.create_abbr grev       git revert
  __brewAbbreviations.create_abbr grh        git reset
  __brewAbbreviations.create_abbr grhh       git reset --hard
  __brewAbbreviations.create_abbr grhpa      git reset --patch
  __brewAbbreviations.create_abbr grm        git rm
  __brewAbbreviations.create_abbr grmc       git rm --cached
  __brewAbbreviations.create_abbr grmv       git remote rename
  __brewAbbreviations.create_abbr grrm       git remote remove
  __brewAbbreviations.create_abbr grs        git restore
  __brewAbbreviations.create_abbr grset      git remote set-url
  __brewAbbreviations.create_abbr grss       git restore --source
  __brewAbbreviations.create_abbr grup       git remote update
  __brewAbbreviations.create_abbr grv        git remote -v
  __brewAbbreviations.create_abbr gsh        git show
  __brewAbbreviations.create_abbr gsd        git svn dcommit
  __brewAbbreviations.create_abbr gsr        git svn rebase
  __brewAbbreviations.create_abbr gss        git status -s
  __brewAbbreviations.create_abbr gst        git status
  __brewAbbreviations.create_abbr gsta       git stash
  __brewAbbreviations.create_abbr gstd       git stash drop
  __brewAbbreviations.create_abbr gstp       git stash pop
  __brewAbbreviations.create_abbr gsts       git stash show --text
  __brewAbbreviations.create_abbr gsu        git submodule update
  __brewAbbreviations.create_abbr gsur       git submodule update --recursive
  __brewAbbreviations.create_abbr gsuri      git submodule update --recursive --init
  __brewAbbreviations.create_abbr gts        git tag -s
  __brewAbbreviations.create_abbr gtv        git tag | sort -V
  __brewAbbreviations.create_abbr gsw        git switch
  __brewAbbreviations.create_abbr gswc       git switch --create
  __brewAbbreviations.create_abbr gunignore  git update-index --no-assume-unchanged
  __brewAbbreviations.create_abbr gup        git pull --rebase
  __brewAbbreviations.create_abbr gwch       git whatchanged -p --abbrev-commit --pretty=medium

  # git checkout abbreviations
  __brewAbbreviations.create_abbr gco        git checkout
  __brewAbbreviations.create_abbr gcb        git checkout -b
  __brewAbbreviations.create_abbr gcod       git checkout develop
  __brewAbbreviations.create_abbr gcom       git checkout \(__brewAbbreviations.default_branch\)

  # git flow abbreviations
  __brewAbbreviations.create_abbr gfb        git flow bugfix
  __brewAbbreviations.create_abbr gff        git flow feature
  __brewAbbreviations.create_abbr gfr        git flow release
  __brewAbbreviations.create_abbr gfh        git flow hotfix
  __brewAbbreviations.create_abbr gfs        git flow support

  __brewAbbreviations.create_abbr gfbs       git flow bugfix start
  __brewAbbreviations.create_abbr gffs       git flow feature start
  __brewAbbreviations.create_abbr gfrs       git flow release start
  __brewAbbreviations.create_abbr gfhs       git flow hotfix start
  __brewAbbreviations.create_abbr gfss       git flow support start

  __brewAbbreviations.create_abbr gfbt       git flow bugfix track
  __brewAbbreviations.create_abbr gfft       git flow feature track
  __brewAbbreviations.create_abbr gfrt       git flow release track
  __brewAbbreviations.create_abbr gfht       git flow hotfix track
  __brewAbbreviations.create_abbr gfst       git flow support track

  __brewAbbreviations.create_abbr gfp        git flow publish

  # Cleanup declared functions
  functions -e __brewAbbreviations.create_abbr

  # Mark git plugin as initialized
  set -U __brewAbbreviations_plugin_initialized (date)
end
function __brewAbbreviations.init
  function __brewAbbreviations.create_abbr -d "Create Git plugin abbreviation"
    set -l name $argv[1]
    set -l body $argv[2..-1]
    abbr -a $name $body
    set -a __brewAbbreviations_plugin_abbreviations $name
  end

  set -q __brewAbbreviations_plugin_initialized; and exit 0

  set -U __brewAbbreviations_plugin_abbreviations

  # git abbreviations
  __brewAbbreviations.create_abbr g          git
  __brewAbbreviations.create_abbr ga         git add
  __brewAbbreviations.create_abbr gaa        git add --all
  __brewAbbreviations.create_abbr gapa       git add --patch
  __brewAbbreviations.create_abbr gap        git apply
  __brewAbbreviations.create_abbr gb         git branch -vv
  __brewAbbreviations.create_abbr gba        git branch -a -v
  __brewAbbreviations.create_abbr gban       git branch -a -v --no-merged
  __brewAbbreviations.create_abbr gbd        git branch -d
  __brewAbbreviations.create_abbr gbD        git branch -D
  __brewAbbreviations.create_abbr gbl        git blame -b -w
  __brewAbbreviations.create_abbr gbs        git bisect
  __brewAbbreviations.create_abbr gbsb       git bisect bad
  __brewAbbreviations.create_abbr gbsg       git bisect good
  __brewAbbreviations.create_abbr gbsr       git bisect reset
  __brewAbbreviations.create_abbr gbss       git bisect start
  __brewAbbreviations.create_abbr gc         git commit -v
  __brewAbbreviations.create_abbr gc!        git commit -v --amend
  __brewAbbreviations.create_abbr gcn!       git commit -v --no-edit --amend
  __brewAbbreviations.create_abbr gca        git commit -v -a
  __brewAbbreviations.create_abbr gca!       git commit -v -a --amend
  __brewAbbreviations.create_abbr gcan!      git commit -v -a --no-edit --amend
  __brewAbbreviations.create_abbr gcv        git commit -v --no-verify
  __brewAbbreviations.create_abbr gcav       git commit -a -v --no-verify
  __brewAbbreviations.create_abbr gcav!      git commit -a -v --no-verify --amend
  __brewAbbreviations.create_abbr gcm        git commit -m
  __brewAbbreviations.create_abbr gcam       git commit -a -m
  __brewAbbreviations.create_abbr gscam      git commit -S -a -m
  __brewAbbreviations.create_abbr gcfx       git commit --fixup
  __brewAbbreviations.create_abbr gcf        git config --list
  __brewAbbreviations.create_abbr gcl        git clone
  __brewAbbreviations.create_abbr gclean     git clean -di
  __brewAbbreviations.create_abbr gclean!    git clean -dfx
  __brewAbbreviations.create_abbr gclean!!   "git reset --hard; and git clean -dfx"
  __brewAbbreviations.create_abbr gcount     git shortlog -sn
  __brewAbbreviations.create_abbr gcp        git cherry-pick
  __brewAbbreviations.create_abbr gcpa       git cherry-pick --abort
  __brewAbbreviations.create_abbr gcpc       git cherry-pick --continue
  __brewAbbreviations.create_abbr gd         git diff
  __brewAbbreviations.create_abbr gdca       git diff --cached
